<div class="card-category"
  style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path list.photo.key, height: 300, crop: :fill %>')"
>
<%= link_to list.name, list_path(list) %>
<%= link_to list_path(list), data: {turbo_method: :delete, turbo_confirm: "Are you sure ?" }, class:"absolute" do %>
          <i class="fa-solid fa-trash"></i>
<% end %>
</div>
